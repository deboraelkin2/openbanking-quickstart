COMPOSE_PROJECT_NAME=openbanking-quickstart

# version of openbanking apps
VERSION=latest

# url where acp is deployed
ACP_URL=https://authorization.cloudentity.com:8443

# url where acp is deployed
ACP_BASE_URL=authorization.cloudentity.com:8443

# internal acp url used to communicate between dockers
# if you deploy acp on a separate host, set this env variable to the same value as ACP_URL
ACP_MTLS_URL=https://authorization.cloudentity.com:8443

# acp version
ACP_VERSION=2.6.0-46af9d7

# host where apps are deployed
APP_HOST=localhost

# acp tenant
TENANT=default

# acp server
SERVER=openbanking

# flag to enable mfa on the consent page
ENABLE_MFA=false

# flag to enable mfa provider - leave blank if not specifying a provider
MFA_PROVIDER=

# Hypr onfiguration
HYPR_TOKEN="<hyper API Token>"
HYPR_BASE_URL="<hypr tenant base URL>"
HYPR_APP_ID="<hypr app id>"

